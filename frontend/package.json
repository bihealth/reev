    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^1.3.1",
    "vitest-canvas-mock": "^0.3.3",
    "vitest-fetch-mock": "^0.2.2",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-tsc": "^2.0.19"
  }
