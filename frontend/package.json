{
  "name": "reev-frontend",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 8081",
    "dev-public": "vite --host 0.0.0.0 --port 8081",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --coverage",
    "test:unit:nocov": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --ignore-pattern 'src/ext/reev-frontend-lib/src/pbs/google/protobuf/*'",
    "format": "prettier --write src/ *.ts *.mts *.json",
    "format:check": "prettier --list-different src/ *.ts *.mts *.json"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@protobuf-ts/runtime": "^2.9.4",
    "@reactgular/chunks": "^1.0.1",
    "@sentry/browser": "^7.114.0",
    "@sentry/integrations": "^7.111.0",
    "@sentry/tracing": "^7.114.0",
    "@sentry/vue": "^7.111.0",
    "@types/luxon": "^3.4.2",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "fast-deep-equal": "^3.1.3",
    "igv": "^2.15.11",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "mitt": "^3.0.1",
    "pinia": "^2.1.7",
    "plotly.js-dist": "^2.32.0",
    "title-case": "^4.3.1",
    "unplugin-fonts": "^1.1.1",
    "vega": "^5.30.0",
    "vega-embed": "^6.25.0",
    "vue": "^3.4.27",
    "vue-matomo": "^4.2.0",
    "vue-router": "^4.3.2",
    "vuetify": "^3.5.11"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rushstack/eslint-patch": "^1.10.3",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.9",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/blocks": "^8.1.1",
    "@storybook/test": "^8.1.11",
    "@storybook/vue3": "^8.3.2",
    "@storybook/vue3-vite": "^8.1.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.14.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitest/coverage-istanbul": "^1.5.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.1.0",
    "npm-check-updates": "^16.14.20",
    "npm-run-all": "^4.1.5",
